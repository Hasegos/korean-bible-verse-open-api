{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://korean-bible-verse-open-api.vercel.app/api/verse"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=5"
        }
      ]
    }
  ]
}